{
  "parser" : "babel-eslint",
  "extends": "airbnb",
  "plugins": [
    "react"
  ],
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "rules": {
    "import/extensions": 0,
    "import/no-mutable-exports": 0,
    "import/prefer-default-export": 0,
    "jsx-a11y/no-static-element-interactions": 0,
    "no-plusplus": [2, { "allowForLoopAfterthoughts": true }],
    "react/no-did-mount-set-state": 0
  },
  "globals": {
    "buildId": true
  }
}
